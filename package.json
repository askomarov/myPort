  "description": "Личный сайт портфолио Askomarov",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "9.8.4",
    "browser-sync": "^2.26.14",
    "gh-pages": "^3.1.0",
    "gulp": "4.0.2",
    "gulp-cheerio": "^1.0.0",
    "gulp-csso": "^4.0.1",
    "gulp-file-include": "^2.2.2",
    "gulp-imagemin": "^7.1.0",
